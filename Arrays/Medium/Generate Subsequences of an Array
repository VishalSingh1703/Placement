#include <iostream>
#include <vector>
#include <bits/stdc++.h>
using namespace std;

void printF(int index, vector<int> &ds, int arr[], int n)
{
    if(index == n)
    {
        for(auto i:ds)
        {
            cout<<i<<" ";
        }
        cout<<endl;
        return;
    }
    
    printF(index+1, ds, arr, n);
    ds.push_back(arr[index]);
    printF(index+1, ds, arr, n);
    ds.pop_back();
}

int main()
{
    int arr[] = {3, 1, 2};
    vector<int> ds;
    printF(0, ds, arr, 3);

    return 0;
}
